  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/dist/*.js"
      ],
      "env": {
        "GITHUB_EVENT_PATH": "path",
        "GITHUB_TOKEN": "path",
        "GITHUB_SHA": "path",
        "GITHUB_WORKSPACE": "path",
        "SPECTRAL_FILE_PATH": "path",
      }
    }
  ]
}
